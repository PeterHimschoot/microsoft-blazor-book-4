@typeparam TItem

<ul>
  @foreach (var item in Items)
  {
    <li>
      @ItemTemplate(item)
    </li>
  }
</ul>

@code {
  [Parameter]
  [EditorRequired]
  public required RenderFragment<TItem> ItemTemplate { get; set; }
  
  [Parameter]
  [EditorRequired]
  public required IReadOnlyList<TItem> Items { get; set; }
}
